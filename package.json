{
  "name": "dropbox-fs",
  "version": "0.3.0",
  "description": "Node FS wrapper for Dropbox API",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "format": "prettier --write '{src,test}/**/*.js'",
    "test": "nyc --require babel-core/register mocha",
    "nyc-report": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sallar/dropbox-fs.git"
  },
  "keywords": [
    "dropbox",
    "fs",
    "node",
    "filesystem",
    "file"
  ],
  "author": "Sallar Kaboli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sallar/dropbox-fs/issues"
  },
  "homepage": "https://github.com/sallar/dropbox-fs#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-stage-3": "^6.24.1",
    "codecov": "^1.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "mocha": "^3.1.2",
    "nyc": "^8.3.1",
    "prettier": "^1.10.2",
    "proxyquire": "^1.8.0",
    "pullout": "^1.0.2",
    "sinon": "^4.2.2",
    "string-to-stream": "^1.1.0",
    "uuid": "^2.0.3"
  },
  "dependencies": {
    "dropbox": "^2.3.0",
    "dropbox-stream": "^2.0.0"
  }
}
